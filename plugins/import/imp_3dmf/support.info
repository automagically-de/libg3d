# supported file types (extensions)
filetypes: 3mf, 3dmf, b3d
# description
description: 
# built without EXPERIMENTAL
stable: [5]
# depends on libraries except glib
dependencies: [5]
# depends on external data files at runtime
depdata: [5]
# libmagic detector
magic: [5]
# G3DStream support
stream: [5]
# Works without seeking
seekfree: [0]
# Progress bar updates
progress: [0]
# Container format
container: [3] ASCII files not supported
# Object grouping
grouping: [3] no hierarchy yet
# Object transformation
transformation: 
# Vertex data
vertex: [4]
# Normal data
normal: 
# Face data
face: [4]
# Face colors
color: [3]
# Face transparency
alpha: [3]
# Texture images
teximage: [0]
# Texture coordinates (UV)
texuv: [0]
# Texture mapping parameters
texmap: [0]
