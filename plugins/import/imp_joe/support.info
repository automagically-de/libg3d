# supported file types (extensions)
filetypes: joe, car
# description
description: VDrift
# built without EXPERIMENTAL
stable: [5]
# depends on libraries except glib
dependencies: [5]
# depends on external data files at runtime
depdata: [5]
# libmagic detector
magic: [3] only for .joe files
# G3DStream support
stream: [5]
# Works without seeking
seekfree: [5]
# Progress bar updates
progress: [0]
# Container format
container: [5]
# Object grouping
grouping: 
# Object transformation
transformation: 
# Vertex data
vertex: 
# Normal data
normal: 
# Face data
face: 
# Face colors
color: 
# Face transparency
alpha: 
# Texture images
teximage: 
# Texture coordinates (UV)
texuv: 
# Texture mapping parameters
texmap: 
