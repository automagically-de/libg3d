# supported file types (extensions)
filetypes: dat, mpd, ldr
# description
description: LDraw
# built without EXPERIMENTAL
stable: [5]
# depends on libraries except glib
dependencies: [5]
# depends on external data files at runtime
depdata: [3] LDraw parts library in $LDRAWDIR
# libmagic detector
magic: [3] partly, .dat has no real magic
# G3DStream support
stream: [5]
# Works without seeking
seekfree: [5]
# Progress bar updates
progress: [3] bigger jumps
# Container format
container: [5]
# Object grouping
grouping: 
# Object transformation
transformation: [5]
# Vertex data
vertex: [5]
# Normal data
normal: 
# Face data
face: [5]
# Face colors
color: [5]
# Face transparency
alpha: [5]
# Texture images
teximage: 
# Texture coordinates (UV)
texuv: 
# Texture mapping parameters
texmap: 
